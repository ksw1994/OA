<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bootdo.oa.dao.LeaveTimeDao">

	<select id="get" resultType="com.bootdo.oa.domain.LeaveTimeDO">
		select `id`,`name`,`dept_name`,`funeral_leave`,`casual_leave`,`marital_leave`,`annual_leave`,`sick_leave`,`rest_can`,`paternity_leave`,`year_month`,`create_by`,`create_time`,`update_by`,`update_time` from oa_leave_time where id = #{value}
	</select>

	<select id="list" resultType="com.bootdo.oa.domain.LeaveTimeDO">
		select `id`,`name`,`dept_name`,`funeral_leave`,`casual_leave`,`marital_leave`,`annual_leave`,`sick_leave`,`rest_can`,`paternity_leave`,`year_month`,`create_by`,`create_time`,`update_by`,`update_time` from oa_leave_time
        <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="name != null and name != ''"> and name = #{name} </if>
		  		  <if test="deptName != null and deptName != ''"> and dept_name = #{deptName} </if>
		  		  <if test="funeralLeave != null and funeralLeave != ''"> and funeral_leave = #{funeralLeave} </if>
		  		  <if test="casualLeave != null and casualLeave != ''"> and casual_leave = #{casualLeave} </if>
		  		  <if test="maritalLeave != null and maritalLeave != ''"> and marital_leave = #{maritalLeave} </if>
		  		  <if test="annualLeave != null and annualLeave != ''"> and annual_leave = #{annualLeave} </if>
		  		  <if test="sickLeave != null and sickLeave != ''"> and sick_leave = #{sickLeave} </if>
		  		  <if test="restCan != null and restCan != ''"> and rest_can = #{restCan} </if>
		  		  <if test="paternityLeave != null and paternityLeave != ''"> and paternity_leave = #{paternityLeave} </if>
		  		  <if test="yearMonth != null and yearMonth != ''"> and `year_month` = #{yearMonth} </if>
		  		  <if test="createBy != null and createBy != ''"> and create_by = #{createBy} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="updateBy != null and updateBy != ''"> and update_by = #{updateBy} </if>
		  		  <if test="updateTime != null and updateTime != ''"> and update_time = #{updateTime} </if>
		  		</where>
        <choose>
            <when test="sort != null and sort.trim() != ''">
                order by ${sort} ${order}
            </when>
			<otherwise>
                order by id desc
			</otherwise>
        </choose>
		<if test="offset != null and limit != null">
			limit #{offset}, #{limit}
		</if>
	</select>
	
 	<select id="count" resultType="int">
		select count(*) from oa_leave_time
		 <where>  
		  		  <if test="id != null and id != ''"> and id = #{id} </if>
		  		  <if test="name != null and name != ''"> and name = #{name} </if>
		  		  <if test="deptName != null and deptName != ''"> and dept_name = #{deptName} </if>
		  		  <if test="funeralLeave != null and funeralLeave != ''"> and funeral_leave = #{funeralLeave} </if>
		  		  <if test="casualLeave != null and casualLeave != ''"> and casual_leave = #{casualLeave} </if>
		  		  <if test="maritalLeave != null and maritalLeave != ''"> and marital_leave = #{maritalLeave} </if>
		  		  <if test="annualLeave != null and annualLeave != ''"> and annual_leave = #{annualLeave} </if>
		  		  <if test="sickLeave != null and sickLeave != ''"> and sick_leave = #{sickLeave} </if>
		  		  <if test="restCan != null and restCan != ''"> and rest_can = #{restCan} </if>
		  		  <if test="paternityLeave != null and paternityLeave != ''"> and paternity_leave = #{paternityLeave} </if>
		  		  <if test="yearMonth != null and yearMonth != ''"> and `year_month` = #{yearMonth} </if>
		  		  <if test="createBy != null and createBy != ''"> and create_by = #{createBy} </if>
		  		  <if test="createTime != null and createTime != ''"> and create_time = #{createTime} </if>
		  		  <if test="updateBy != null and updateBy != ''"> and update_by = #{updateBy} </if>
		  		  <if test="updateTime != null and updateTime != ''"> and update_time = #{updateTime} </if>
		  		</where>
	</select>
	 
	<insert id="save" parameterType="com.bootdo.oa.domain.LeaveTimeDO">
		insert into oa_leave_time
		(
			`id`, 
			`name`, 
			`dept_name`, 
			`funeral_leave`, 
			`casual_leave`, 
			`marital_leave`, 
			`annual_leave`, 
			`sick_leave`, 
			`rest_can`, 
			`paternity_leave`, 
			`year_month`, 
			`create_by`, 
			`create_time`, 
			`update_by`, 
			`update_time`
		)
		values
		(
			#{id}, 
			#{name}, 
			#{deptName}, 
			#{funeralLeave}, 
			#{casualLeave}, 
			#{maritalLeave}, 
			#{annualLeave}, 
			#{sickLeave}, 
			#{restCan}, 
			#{paternityLeave}, 
			#{yearMonth}, 
			#{createBy}, 
			#{createTime}, 
			#{updateBy}, 
			#{updateTime}
		)
	</insert>
	 
	<update id="update" parameterType="com.bootdo.oa.domain.LeaveTimeDO">
		update oa_leave_time 
		<set>
			<if test="name != null">`name` = #{name}, </if>
			<if test="deptName != null">`dept_name` = #{deptName}, </if>
			<if test="funeralLeave != null">`funeral_leave` = #{funeralLeave}, </if>
			<if test="casualLeave != null">`casual_leave` = #{casualLeave}, </if>
			<if test="maritalLeave != null">`marital_leave` = #{maritalLeave}, </if>
			<if test="annualLeave != null">`annual_leave` = #{annualLeave}, </if>
			<if test="sickLeave != null">`sick_leave` = #{sickLeave}, </if>
			<if test="restCan != null">`rest_can` = #{restCan}, </if>
			<if test="paternityLeave != null">`paternity_leave` = #{paternityLeave}, </if>
			<if test="yearMonth != null">`year_month` = #{yearMonth}, </if>
			<if test="createBy != null">`create_by` = #{createBy}, </if>
			<if test="createTime != null">`create_time` = #{createTime}, </if>
			<if test="updateBy != null">`update_by` = #{updateBy}, </if>
			<if test="updateTime != null">`update_time` = #{updateTime}</if>
		</set>
		where id = #{id}
	</update>
	
	<delete id="remove">
		delete from oa_leave_time where id = #{value}
	</delete>
	
	<delete id="batchRemove">
		delete from oa_leave_time where id in 
		<foreach item="id" collection="array" open="(" separator="," close=")">
			#{id}
		</foreach>
	</delete>

</mapper>